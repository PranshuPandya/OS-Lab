+#include "user.h"
+#include "types.h"
+#include "stat.h"
+#include "processInfo.h"
+int main(int argc, char *argv[])
+{
+	// static char buf[2000];
+	// printf(1,"drawtest system call returns %d\n",draw((void*)buf,2000));
+	printf(1, "\ngetNumProc\n-----------------\n");
+	printf(1, "The number of process: %d\n", getNumProc());
+	printf(1, "\ngetMaxPid\n-----------------\n");
+	printf(1, "The maxpid of till now: %d\n", getMaxPid());
+	// printf(1,"%s",buf);
+	// int *arr = getProcessTable();
+	// for(int i=0;i<64;i++){
+	//     printf(1,"%d\n",arr[i]);
+	// }
+	// argv[0] is program name and argv[1][0] contains the process id from command line.
+	int pid = atoi(argv[1]);
+	set_burst_time(5);
+
+	// declaring the user memory space for a process
+	struct processInfo *p = malloc(sizeof(struct processInfo));
+
+	// getProcInfo() system call is used from user.h which returns 0 if required process is found in the system and -1 otherwise.
+	int value = getProcInfoStruct(pid, p);
+
+	printf(1, "\nNumber of Context Switches\n------------------------------------\n");
+	if (value == -1)
+	{
+		printf(1, "Process Not Found\n");
+	}
+	else
+	{
+		// printing the information for the required process.
+		printf(1, "Process ID: %d\nProcess Size: %d\nNumber of Context Switches: %d\n", p->ppid, p->psize, p->numberContextSwitches);
+	}
+	printf(1, "\nBurst time of the current process: %d\n", get_burst_time());
+	exit();
+}
